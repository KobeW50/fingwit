#: data/fingwit.desktop.in:3 fingwit:80 data/fingwit/fingwit.ui.h:9
msgid "Fingerprints"
msgstr "Impressões digitais"

#: data/fingwit.desktop.in:4 fingwit:133 data/fingwit/fingwit.ui.h:10
msgid "Fingerprint configuration tool"
msgstr "Ferramenta de configuração de impressões digitais"

#: data/fingwit.desktop.in:11
msgid "fingerprint;authentication;security;biometric;"
msgstr "impressão digital;autenticação;segurança;biométrica;"

#: fingwit:39
msgid "Right index finger"
msgstr "Indicador direito"

#: fingwit:40
msgid "Right middle finger"
msgstr "Dedo do meio direito"

#: fingwit:41
msgid "Right ring finger"
msgstr "Dedo anelar direito"

#: fingwit:42
msgid "Right little finger"
msgstr "Dedo mindinho direito"

#: fingwit:43
msgid "Right thumb"
msgstr "Polegar direito"

#: fingwit:44
msgid "Left index finger"
msgstr "Indicador esquerdo"

#: fingwit:45
msgid "Left middle finger"
msgstr "Dedo do meio esquerdo"

#: fingwit:46
msgid "Left ring finger"
msgstr "Dedo anelar esquerdo"

#: fingwit:47
msgid "Left little finger"
msgstr "Dedo mindinho esquerdo"

#: fingwit:48
msgid "Left thumb"
msgstr "Polegar esquerdo"

#: fingwit:97
msgid "Disable fingerprint authentication"
msgstr "Desativar a autenticação por impressão digital"

#: fingwit:101 fingwit:131
msgid "About"
msgstr "Acerca"

#: fingwit:106
msgid "Quit"
msgstr "Sair"

#: fingwit:272
msgid "Place your finger on the scanner..."
msgstr "Coloque o dedo no leitor..."

#: fingwit:290
msgid "Well done! Your fingerprint was saved successfully."
msgstr "Muito bem! A sua impressão digital foi guardada com sucesso."

#: fingwit:296
#, python-format
msgid "Good scan (%d so far)! Do it again..."
msgstr "Boa leitura (%d até agora)! Repita..."

#: fingwit:299
msgid "Try again..."
msgstr "Tente novamente..."

#: fingwit:302
msgid "Sorry, your fingerprint could not be saved."
msgstr "Lamentamos, mas não foi possível guardar a sua impressão digital."

#: data/fingwit/fingwit.ui.h:1
msgid "No fingerprint reader detected"
msgstr "Não foi detetado nenhum leitor de impressões digitais"

#: data/fingwit/fingwit.ui.h:2
msgid "Fingerprint authentication disabled"
msgstr "Autenticação de impressões digitais desativada"

#: data/fingwit/fingwit.ui.h:3
msgid "Enable"
msgstr "Ativar"

#: data/fingwit/fingwit.ui.h:4
msgid "Choose a finger to configure it:"
msgstr "Escolha um dedo para o configurar:"

#: data/fingwit/fingwit.ui.h:5
msgid "Active fingerprints are highlighted in green."
msgstr "As impressões digitais ativas estão destacadas a verde."

#: data/fingwit/fingwit.ui.h:6
msgid "Cancel"
msgstr "Cancelar"

#: data/fingwit/fingwit.ui.h:7
msgid "Delete this fingerprint"
msgstr "Eliminar esta impressão digital"

#: data/fingwit/fingwit.ui.h:8
msgid "Add a fingerprint"
msgstr "Adicionar uma impressão digital"
